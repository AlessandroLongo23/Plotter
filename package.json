{
	"name": "stochastic-simulation",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:websocket": "concurrently \"npm run server\" \"npm run dev\"",
		"dev:http": "concurrently \"npm run api-server\" \"npm run dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"server": "python start_server.py",
		"api-server": "node dev-server.js",
		"start": "npm run dev:http",
		"deploy": "vercel --prod",
		"vercel-dev": "vercel dev"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"concurrently": "^8.2.2",
		"svelte": "^5.0.0",
		"tailwindcss": "^4.0.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"express": "^5.1.0",
		"lucide-svelte": "^0.514.0",
		"p5": "^2.0.3"
	}
}
